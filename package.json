{
  "name": "luos",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ]
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,mdx}\""
  },
  "devDependencies": {
    "eslint": "^8.31.0",
    "eslint-config-custom": "workspace:^1.0.0",
    "prettier": "^2.8.2",
    "turbo": "^1.7.0",
    "typescript": "^4.9.4"
  },
  "engines": {
    "pnpm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "dependencies": {
    "dotenv-cli": "^7.1.0",
    "dotenv-vault": "^1.17.0"
  },
  "packageManager": "pnpm@7.13.5"
}
